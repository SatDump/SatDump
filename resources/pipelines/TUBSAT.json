                    "rrc_alpha": 0.25,
                    "pll_bw": 0.002
                }
            },
            "cadu": {
                "module": "ccsds_conv_concat_decoder",
                "parameters": {
                    "constellation": "qpsk",
                    "cadu_size": 4416,
                    "viterbi_ber_thresold": 0.300,
                    "viterbi_outsync_after": 20,
                    "derandomize": true,
                    "rs_i": 0
                }
            },
            "products": {
                "module": "tubin_decoder",
                "parameters": {
                    "check_crc": true
                }
            }
        }
