        ],
        "parameters": {
            "sstv_mode": {
                "type": "options",
                "value": "M2-2",
                "name": "SSTV Mode",
                "options": [
                    "BW8",
                    "FAX480",
                    "Robot36",
                    "PD120"
                ],
                "description": "Please select the desired SSTV Mode!"
            },
            "start_timestamp": {
                "type": "timestamp",
                "value": -1,
                "name": "Start Timestamp",
                "description": "Unix timestamp of the start of the file provided.\nRequired for doppler correction, along with your QTH.\n\nIf your baseband filename is a support format it will be read automatically."
            },
            "enable_doppler": {
                "type": "bool",
                "value": false,
                "name": "Doppler Correction",
                "description": "Apply doppler correction"
            },
            "satellite_norad": {
                "type": "int",
                "value": 25544,
                "name": "Satellite NORAD Number",
                "description": "NORAD number identifying the satellite to correct the Doppler effect for.\nDefaults to ISS's NORAD ID."
            },
            "satellite_frequency": {
                "type": "notated_int",
                "value": 435.87e6,
                "name": "Satellite Nominal Frequency",
                "units": "Hz",
                "description": "Downlink frequency of the satellite."
            },
            "satellite_name": {
                "type": "string",
                "value": "",
                "name": "Satellite Name",
                "description": "Human-readable identifier for the satellite."
            },
            "modulation_type": {
                "type": "options",
                "value": "NFM",
                "options": [
                    "AM",
                    "NFM"
                ],
                "name": "Modulation Technique",
                "description": "The analog modulation technique to demodulate."
            },
            "bandwidth": {
                "type": "notated_int",
                "value": 12500,
                "name": "Bandwidth",
                "units": "Hz",
                "description": "Signal's bandwidth to be used in the demodulation process."
            },
            "record_demod_audio": {
                "type": "bool",
                "value": false,
                "name": "Record Audio from Demodulation",
                "description": "Write the resulting audio signal after demodulation to a file."
            }
        },
        "work": {
            "baseband": {},
            "audio_wav": {
                "module": "generic_analog_demod",
                "parameters": {
                    "symbolrate": 12e3
                }
            },
            "products": {
                "module": "sstv_decoder",
                "parameters": {
                    "audio_samplerate": 48e3
                }
            }
        }
    },
