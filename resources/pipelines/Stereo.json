{
    "stereo_lr": {
        "name": "Stereo-A/B Low Rate",
        "live": [
            1,
            2
        ],
        "work": {
            "baseband": {},
            "soft": {
                "module": "pm_demod",
                "parameters": {
                    "symbolrate": 3800,
                    "resample_after_pll": true,
                    "agc_rate": 0.001,
                    "pll_bw": 0.01,
                    "pll_max_offset": 3.14,
                    "rrc_alpha": 0.6,
                    "rrc_taps": 31,
                    "costas_bw": 0.005,
                    "clock_omega_relative_limit": 0.005
                }
            },
            "cadu": {
                "module": "ccsds_turbo_decoder",
                "parameters": {
                    "turbo_rate": "1/6",
                    "turbo_base": 1115,
                    "turbo_iters": 20
                }
            },
            "products": {
                "module": "stereo_instruments",
                "parameters": {
                    "icer_path": "./icer_decomp_linux"
                }
            }
        }
    },
    "stereo_hr": {
        "name": "Stereo-A/B High Rate",
        "live": [
            1,
            2
        ],
        "work": {
            "baseband": {},
            "soft": {
                "module": "psk_demod",
                "parameters": {
                    "constellation": "bpsk",
                    "symbolrate": 1.44e6,
                    "rrc_alpha": 0.35,
                    "pll_bw": 0.002,
                    "post_costas_dc": true
                }
            },
            "cadu": {
                "module": "ccsds_turbo_decoder",
                "parameters": {
                    "turbo_rate": "1/2",
                    "turbo_base": 1115,
                    "turbo_iters": 1
                }
            },
            "products": {
                "module": "stereo_instruments",
                "parameters": {
                    "icer_path": "./icer_decomp_linux"
                }
            }
        }
    }
}