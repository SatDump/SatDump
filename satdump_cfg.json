                        "autogen": false
                    },
                    "Snow enhancement": {
                        "equation": "((3*ch1-ch4)/2)^1.7, ((3*ch1+ch4)/4)^1.7, (ch1-ch3a)^1.7",
                        "equalize": true,
                        "white_balance": true,
                        "geo_correct": true,
                        "autogen": false
                    },
                    "NDVI (Vegetation)": {
                        "equation": "((cch2-cch1)/(cch2+cch1)+1)/2",
                        "geo_correct": true,
                        "autogen": false
                    },
                    "WXtoImg NO": {
