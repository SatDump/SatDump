                            "height": 2048,
                            "old_algo": true,
                            "draw_map": true,
                            "config": {
                                "type": "equirectangular"
                            }
                        }
                    },
                    "Thermal Channel": {
                        "equation": "cch4",
                        "equalize": true,
                        "geo_correct": true
                    },
                    "Night Microphysics": {
                        "equation": "cch5-cch4,cch4-cch3b,cch4",
                        "geo_correct": true,
                        "equalize": true,
                        "white_balance": true
                    },
                    "Day Microphysics": {
                        "equation": "cch1, cch3b^2.5,cch4",
                        "geo_correct": true,
                        "equalize": true
                    },
                    "Clouds Underlay (Merge)": {
                        "channels": "ch4",
                        "lua": "scripted_compos/underlay_with_clouds.lua",
                        "lua_vars": {
                            "minoffset": 0.4,
                            "scalar": 4.0,
                            "thresold": 0,
                            "blend": 0
