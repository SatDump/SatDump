{
    "meteor_hrpt": {
        "name": "METEOR HRPT",
        "live": true,
        "live_cfg": [
            [
                1,
                0
            ],
            [
                2,
                0
            ]
        ],
        "frequencies": [
            [
                "Main",
                1700e6
            ]
        ],
        "parameters": {
            "samplerate": {
                "value": 3e6
            },
            "start_timestamp": {
                "type": "timestamp",
                "value": -1,
                "name": "Start Timestamp",
                "description": "Unix timestamp of the start of the file provided.\nRequired in case you are not\nprocessing your file on the same Mocow day."
            }
        },
        "work": {
            "baseband": {},
            "soft": {
                "pm_demod": {
                    "symbolrate": 665400,
                    "pll_bw": 0.01,
                    "rrc_alpha": 0.5,
                    "clock_omega_relative_limit": 0.001
                }
            },
            "cadu": {
                "meteor_hrpt_decoder": {}
            },
            "products": {
                "meteor_instruments": {}
            }
        }
    },
    "meteor_m2_lrpt": {
        "name": "METEOR M2 LRPT 72k",
        "live": true,
        "live_cfg": [
            [
                1,
                0
            ],
            [
                2,
                0
            ]
        ],
        "frequencies": [
            [
                "Primary",
                137.1e6
            ],
            [
                "Backup",
                137.9e6
            ]
        ],
        "parameters": {
            "samplerate": {
                "value": 1e6
            },
            "fill_missing": {
                "type": "bool",
                "value": false,
                "name": "Fill Missing Data",
                "description": "Creates \"MSU-MR (Filled)\" folder, which corrects black lines\ncaused by interference or signal drop-outs"
            }
        },
        "work": {
            "baseband": {},
            "soft": {
                "psk_demod": {
                    "constellation": "qpsk",
                    "symbolrate": 72e3,
                    "rrc_taps": 31,
                    "rrc_alpha": 0.6,
                    "pll_bw": 0.002
                }
            },
            "cadu": {
                "meteor_lrpt_decoder": {
                    "diff_decode": false
                }
            },
            "products": {
                "meteor_msumr_lrpt": {
                    "m2x_mode": false,
                    "max_fill_lines": 50
                }
            }
        }
    },
