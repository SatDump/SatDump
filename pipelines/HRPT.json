            1702.5
        ],
        "samplerate": 3e6,
        "baseband_type": "i16",
        "work": {
            "baseband": {},
            "soft": {
                "pm_demod": {
                    "symbolrate": 665400,
                    "agc_rate": 0.002e-3,
                    "pll_bw": 0.01,
                    "pll_max_offset": 0.5,
                    "rrc_alpha": 0.6,
                    "rrc_taps": 31,
                    "costas_bw": 0.005,
                    "clock_omega_relative_limit": 0.005,
                    "buffer_size": 8192
                }
            },
            "frames": {
                "noaa_hrpt_decoder": {
                    "deframer_thresold": 10
                }
            },
            "products": {
                "noaa_avhrr": {
                    "composites": {
                        "RGB-221": {
                            "expression": "ch2, ch2, ch1",
                            "corrected": true
                        },
                        "RGB-221-EQU": {
                            "expression": "ch2, ch2, ch1",
                            "equalize": true,
                            "normalize": true,
                            "corrected": true,
                            "projected": true
                        },
                        "RGB-(4)221": {
                            "expression": "ch1 < 0.065 ? (ch4 - 0.7) * 2.66 : ch2 * 2.2 - 0.15, ch1 < 0.065 ? (ch4 - 0.7) * 2.66 : ch2 * 2.2 - 0.15, ch1 < 0.065 ? (ch4 - 0.7) * 2.66 : ch1 * 2.2 - 0.15",
                            "corrected": true
                        },
                        "4-EQU": {
                            "expression": "ch4",
                            "equalize": true,
                            "normalize": true,
                            "corrected": true,
                            "projected": true
                        },
                        "RGB-124": {
                            "expression": "ch1, ch2, ch4",
                            "corrected": true
                        },
                        "RGB-124-EQU": {
                            "expression": "ch1, ch2, ch4",
                            "equalize": true,
                            "normalize": true,
                            "corrected": true,
                            "projected": true
                        }
                    }
                },
                "noaa_extractor": {},
                "noaa_hirs": {
                    "input_override": "noaa_hrpt.tip"
                },
                "noaa_amsu": {
                    "input_override": "noaa_hrpt.aip"
                },
                "noaa_mhs": {
                    "input_override": "noaa_hrpt.aip"
                }
            }
        }
